{
    "name": "enovatedesign/craft-rollbar",
    "description": "Report Craft errors and exceptions to Rollbar.",
    "version": "1.0.2",
    "type": "craft-plugin",
    "minimum-stability": "dev",
    "require": {
        "craftcms/cms": "^3.0.0",
        "rollbar/rollbar": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "enovatedesign\\rollbar\\": "src/"
        }
    },
    "support": {
        "email": "support@enovate.co.uk",
        "issues": "https://github.com/enovatedesign/craft-rollbar/issues"
    },
    "extra": {
        "name": "Rollbar",
        "handle": "rollbar",
        "developer": "Enovate Design",
        "developerUrl": "https://www.enovate.co.uk",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/enovatedesign/craft-rollbar/v1/CHANGELOG.md"
    }
}
